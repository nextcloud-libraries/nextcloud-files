    "core-js": "3.6.4"
  },
  "devDependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-typescript": "7.8.3",
    "@nextcloud/browserslist-config": "^1.0.0",
    "@nextcloud/typings": "0.1.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "typedoc": "^0.16.7",
    "typescript": "3.7.5"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ]
