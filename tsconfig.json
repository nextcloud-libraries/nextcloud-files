{
  "include": ["lib"],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "module": "ESNext",
    "declaration": true,
    "strict": true,
    "noImplicitAny": false,
    "outDir": "./dist",
    "rootDir": "./lib",
    "noEmit": true,
  }
}
